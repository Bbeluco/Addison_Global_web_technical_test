{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@customTypes/*":[
        "src/types/*"
      ],
      "@constants": [
        "src/constants"
      ],
      "@customUtils/*": [
        "src/utils/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ]
    }
  }
}
